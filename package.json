    "simple-syntax-highlighter": "3.0.2",
    "vue": "3.3.11",
    "vue-material": "^1.0.0-beta-16",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.6.0",
    "@tsconfig/node18": "18.2.2",
    "@types/node": "20.10.4",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.4.0",
    "cpy-cli": "5.0.0",
    "eslint": "8.55.0",
    "eslint-plugin-vue": "9.19.2",
    "npm-run-all2": "^6.1.1",
    "typescript": "5.3.3",
